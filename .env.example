## PHP ROOT PATH
## 开发环境 PHP 项目文件路径（绝对路径）

LNMP_PHP_PATH=/app
# LNMP_PHP_PATH=/Users/khs1994/khs1994-lnmp/app

NGINX_CONF_GIT="-b master https://github.com/khs1994-docker/lnmp-nginx-conf-demo.git"

HTTPD_CONF_GIT="-b master https://github.com/khs1994-docker/lnmp-httpd-conf-demo.git"

WINDOWS_NGINX_CONF_GIT="-b windows https://github.com/khs1994-docker/lnmp-nginx-conf-demo.git"

WINDOWS_HTTPD_CONF_GIT="-b windows https://github.com/khs1994-docker/lnmp-httpd-conf-demo.git"

WINDOWS_NGINX_CONF_GIT=

WINDOWS_HTTPD_CONF_GIT=

MACOS_NGINX_CONF_GIT=

MACOS_HTTPD_CONF_GIT=

