#
# Docker Compose
#

COMPOSE_PROJECT_NAME=lnmp

# COMPOSE_PROJECT_NAME=my_lnmp

#
# Project Domain
#

LNMP_DOMAIN=com.khs1994.lnmp

# LNMP_DOMAIN=com.domain.lnmp

#
# 设置项目根目录，以便在任意目录执行该脚本
#

LNMP_PATH=
# LNMP_PATH=~/lnmp
# LNMP_PATH=~/khs1994-lnmp
# LNMP_PATH=/data/lnmp
# LNMP_PATH=/home/khs1994/lnmp

#
# ClusterKit
#

CLUSTERKIT_REDIS_HOST=
CLUSTERKIT_REDIS_S_HOST=
CLUSTERKIT_REDIS_M_S_HOST=

CLUSTERKIT_MYSQL_HOST=

#
# DNS
#

DNS=114.114.114.114
# DNS=8.8.8.8

#
# SSL Powered By acme.sh
#

DNS_TYPE=dns_dp
# DNS_TYPE=

## [DNSPOD]

DP_Id=
DP_Key=

#
# NGINX
#

NGINX_HTTPS_PORT=0.0.0.0:443
NGINX_HTTP_PORT=0.0.0.0:80

#
# HTTPD
#

HTTPD_HTTPS_PORT=0.0.0.0:443
HTTPD_HTTP_PORT=0.0.0.0:80

#
# MySQL MariaDB
#

MYSQL_PORT=127.0.0.1:3306
# MYSQL_PORT=127.0.0.1:3306

#
# Redis
#

REDIS_PORT=127.0.0.1:6379
# REDIS_PORT=127.0.0.1:6379

#
# Memcached
#

MEMCACHED_PORT=127.0.0.1:11211
# MEMCACHED_PORT=127.0.0.1:11211

#
# PostgreSQL
#

POSTGRESQL_PORT=127.0.0.1:5432
# POSTGRESQL_PORT=127.0.0.1:5432

#
# MongoDB
#

MONGODB_PORT=127.0.0.1:27017
# MONGODB_PORT=127.0.0.1:27017

#
# PHPMyAdmin
#

PHPMYADMIN_PORT=127.0.0.1:8080
# PHPMYADMIN_PORT=127.0.0.1:8080

#
# RabbitMQ
#

RABBITMQ_PORT_1000=127.0.0.1:15672
RABBITMQ_PORT=127.0.0.1:5672

#
# TimeZone
#

TZ=Asia/Shanghai
# TZ=Asia/Shanghai

#
# Docker Image ARM Tag
#

ARM_PHP_BASED_OS=

ARM_BASED_OS=-alpine

ARM_ARCH=

#
# [Docker Registry]
#

KHS1994_LNMP_REGISTRY_HOST=docker.t.khs1994.com
# KHS1994_LNMP_REGISTRY_HOST=docker.t.khs1994.com

#
# Soft Version
#

KHS1994_LNMP_NGINX_VERSION=1.13.12

KHS1994_LNMP_HTTPD_VERSION=2.4.33

KHS1994_LNMP_MYSQL_VERSION=8.0.11

KHS1994_LNMP_MARIADB_VERSION=10.3.5

KHS1994_LNMP_REDIS_VERSION=4.0.9

KHS1994_LNMP_PHP_VERSION=7.2.4

KHS1994_LNMP_MEMCACHED_VERSION=1.5.6

KHS1994_LNMP_RABBITMQ_VERSION=3.7.3

KHS1994_LNMP_POSTGRESQL_VERSION=10.3

KHS1994_LNMP_MONGODB_VERSION=3.7.3

KHS1994_LNMP_REGISTRY_VERSION=latest

KHS1994_LNMP_PHPMYADMIN_VERSION=latest

#
# CONF FILE NAME
#
# 复制示例配置文件，并在最后一个点之前加上 .my 前缀
#

# LNMP_MYSQL_CONF=docker.my.cnf

# LNMP_MYSQL_DATA=/var/lib/mysql-my

# DB_ROOT_PASSWORD_PATH=db.my.txt

# LNMP_MARIADB_CONF=docker.my.cnf

# LNMP_MARIADB_DATA=/var/lib/mysql-my

# REDIS_CONF=redis.my.cnf

# MONGODB_CONF=mongod.my.conf

# LNMP_PHP_FPM_CONF=zz-docker.my.conf

# LNMP_PHP_INI=php.development.my.ini

# LNMP_PHP_XDEBUG_INI=xdebug.my.ini

# LNMP_PHP_ERROR_INI=error-log.my.ini

#
# 文件夹
#

# NGINX_CONF_D=nginx.my

# NGINX_CONF=nginx.my.conf

# HTTPD_CONF=httpd.my.conf

#
# 文件夹
#

# HTTPD_CONF_D=httpd.my

# REGISTRY_CONF=config.my.yml

#
# [Development]
# 开发环境 Docker 镜像前缀
#

LNMP_DOCKER_IMAGE_PREFIX=khs1994
# LNMP_DOCKER_IMAGE_PREFIX=username

LNMP_SELF_BUILD_DOCKER_IMAGE_PREFIX=khs1994
# LNMP_SELF_BUILD_DOCKER_IMAGE_PREFIX=khs1994

#
## PHP ROOT PATH
## 开发环境 PHP 项目文件路径（绝对路径）
#

LNMP_PHP_PATH=/app
# LNMP_PHP_PATH=/Users/khs1994/khs1994-lnmp/app

NGINX_CONF_GIT="-b master https://github.com/khs1994-docker/lnmp-nginx-conf-demo.git"

HTTPD_CONF_GIT="-b master https://github.com/khs1994-docker/lnmp-httpd-conf-demo.git"

DEVELOPMENT_INCLUDE="nginx mysql php7 redis phpmyadmin"

#
# Available soft
#

# DEVELOPMENT_INCLUDE="nginx mysql php7 redis mariadb memcached rabbitmq postgresql mongodb phpmyadmin httpd registry"

########################## Don't Edit #############################

KHS1994_LNMP_DOCKER_VERSION=
