COMPOSE_PROJECT_NAME=lnmp

APP_ENV=development

# KhsCI conf

CI_HOST=
# CI_HOST=ci.khs1994.com:10000

# Project Domain

LNMP_DOMAIN=com.khs1994.lnmp

# LNMP_DOMAIN=com.domain.lnmp

# 设置项目根目录，以便在任意目录执行该脚本

LNMP_PATH=
# LNMP_PATH=~/lnmp
# LNMP_PATH=~/khs1994-lnmp
# LNMP_PATH=/data/lnmp
# LNMP_PATH=/home/khs1994/lnmp

# ClusterKit

CLUSTERKIT_REDIS_HOST=
CLUSTERKIT_REDIS_NODES=
# CLUSTERKIT_REDIS_NODES=192.168.199.100:7001 192.168.199.100:7002 192.168.199.100:7003 192.168.199.100:8001 192.168.199.100:8002 192.168.199.100:8003

CLUSTERKIT_REDIS_S_HOST=
CLUSTERKIT_REDIS_M_S_HOST=

CLUSTERKIT_MYSQL_HOST=

DNS=114.114.114.114
# DNS=8.8.8.8

# SSL Powered By acme.sh

DNS_TYPE=dns_dp
# DNS_TYPE=

## [DNSPOD]

DP_Id=
DP_Key=

# NGINX

NGINX_HTTPS_PORT=0.0.0.0:443
NGINX_HTTP_PORT=0.0.0.0:80

# HTTPD

HTTPD_HTTPS_PORT=0.0.0.0:443
HTTPD_HTTP_PORT=0.0.0.0:80

# MySQL MariaDB

MYSQL_PORT=127.0.0.1:3306
# MYSQL_PORT=127.0.0.1:3306

# Redis

REDIS_PORT=127.0.0.1:6379
# REDIS_PORT=127.0.0.1:6379

# Memcached

MEMCACHED_PORT=127.0.0.1:11211
# MEMCACHED_PORT=127.0.0.1:11211

# PostgreSQL

POSTGRESQL_PORT=127.0.0.1:5432
# POSTGRESQL_PORT=127.0.0.1:5432

# MongoDB

MONGODB_PORT=127.0.0.1:27017
# MONGODB_PORT=127.0.0.1:27017

# PHPMyAdmin

PHPMYADMIN_PORT=127.0.0.1:8080
# PHPMYADMIN_PORT=127.0.0.1:8080

# RabbitMQ

RABBITMQ_PORT_1000=127.0.0.1:15672
RABBITMQ_PORT=127.0.0.1:5672

# Gogs

LNMP_GOGS_SSH_PORT=8022
LNMP_GOGS_HTTP_PORT=3000
LNMP_GOGS_SSL_PATH=config/nginx/demo-ssl

# GitLab

LNMP_GITLAB_HOST=
# LNMP_GITLAB_HOST=https://git.t.khs1994.com

LNMP_GITLAB_SSH_PORT=8022

# LNMP_GITLAB_HTTPS_PORT

# please use https
# LNMP_GITLAB_HTTP_PORT

# TimeZone

TZ=Asia/Shanghai
# TZ=Asia/Shanghai

# Docker Image ARM Tag

ARM_PHP_BASED_OS=

ARM_BASED_OS=-alpine

ARM_ARCH=

# [Docker Registry]

KHS1994_LNMP_REGISTRY_HOST=docker.t.khs1994.com
# KHS1994_LNMP_REGISTRY_HOST=docker.t.khs1994.com

# Soft Version

KHS1994_LNMP_NGINX_VERSION=1.15.1

KHS1994_LNMP_NGINX_UNIT_VERSION=1.3

KHS1994_LNMP_HTTPD_VERSION=2.4.34

KHS1994_LNMP_MYSQL_VERSION=8.0.11

KHS1994_LNMP_MARIADB_VERSION=10.3.7

KHS1994_LNMP_REDIS_VERSION=5.0-rc3

KHS1994_LNMP_PHP_VERSION=7.2.8

KHS1994_LNMP_MEMCACHED_VERSION=1.5.9

KHS1994_LNMP_RABBITMQ_VERSION=3.7.6

KHS1994_LNMP_POSTGRESQL_VERSION=10.4

KHS1994_LNMP_MONGODB_VERSION=4.0.0

KHS1994_LNMP_REGISTRY_VERSION=latest

KHS1994_LNMP_PHPMYADMIN_VERSION=latest

